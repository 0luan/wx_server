package main

import (
	_ "proj/routers"
	"github.com/astaxie/beego"
	"github.com/astaxie/beego/orm"
	_"github.com/go-sql-driver/mysql"

)

type Content struct {
	id int
	category_id int
	title string
	content string
}

func init() {
	orm.RegisterDataBase("default", "mysql", "root:stud2in.@/test?charset=utf8", 10)
}

func main() {
	orm.Debug = true
	o := orm.NewOrm()
	o.Using("default")
	
	tmp := Content {category_id: 5, title:"title", content:"content"}
	num, err := o.Insert(&tmp)


	beego.Run()
}

